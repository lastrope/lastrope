/** INFORMATION DATABASE **/
ENCODING : utf8_unicode_ci

/** TABLE LINKS **/
idLinks = id autoincrémenté
text    = Texte du lien afficher en front
href    = Destination du lien en http://
lang    = Tag de langue 

/** TABLE HEADER **/
idHeader    = id autoincrémenté
title       = Titre du site 
meta_description = Balise meta description
meta_keywords    = Balise meta keywords
lang             = Tag de langue

/** TABLE MEMBERS **/
idMembers		= id autoincrémenté
name			= nom de famille
firstname		= prénom
surname			= pseudos, surnoms
picture			= image associé ( ex: kiki.png ) 
birthday		= date de naissance
instrument		= date de naissance
influences		= date de naissance
short_desc		= date de naissance
lang			= date de naissance

/** TABLE ACTUS **/
idActus     = id autoincrémenté
title       = titre de l'actu
body        = corps de l'actu
date        = int(11) pour recevoir le timestamp de la fonction time() 
lang		= varchar(2)

/** TABLE EVENT **/
id     		= id autoincrémenté
title       = titre de l'event
short_desc  = corps de l'event
date        = varchar(11)
type		= varchar(11) 
lang		= varchar(2)